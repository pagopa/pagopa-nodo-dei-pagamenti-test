    # stato3 == 'RPT_ANNULLATA_WISP'
    
    #RT_VERSAMENTI
    # id != null
    # progressivo == 1
    # importoRT == 0
    # esito == 'BATCH_ANNULLAMENTO:TOKEN_SCADUTO'
    # causaleVersamento == RPT_VERSAMENTI.causale_versamento
    # datiSpecificiRiscossione == RPT_VERSAMENTI.dati_specifici_riscossione
    # commissione == 0
    # fkRT == RT.id
    # insertedTimestamp != null
    # updatedTimestamp != null
    
    #RT_XML
    # id != null
    # ccp == RPT.ccp
    # identDominio == RPT.ident_dominio
    # iuv1 == RPT.iuv
    # fkRT == RT.id
    # tipoFirma == null
    # xmlContent != null
    # isertedTimestamp != null
    # updatedTimestamp != null
    # idSessione == RT.id_sessione