    "dict_values = {}\n",
    "new_indeces = initial_indices\n",
    "\n",
    "for field in match:\n",
    "    if j > 0:\n",
    "        new_indeces = []\n",
    "        indices = [i for i, x in enumerate(body) if x == \"$\"]\n",
    "        new_indeces = indices[(4*j):] \n",
    "\n",
    "    dict_values.update({field.replace('$', '').strip() : new_indeces[0]})\n",
