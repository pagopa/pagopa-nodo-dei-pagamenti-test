<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:ws="http://ws.pagamenti.telematici.gov/">
	<soapenv:Header/>
	<soapenv:Body>
		<ws:paaInviaRTRisposta>
			<paaInviaRTRisposta>
				<!--Optional:-->
				<fault>
					<faultCode>$faultCode</faultCode>
					<faultString>$faultString</faultString>
					<id>$id</id>
